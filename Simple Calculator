n=int(input("ENTER THE NUMBER:"))
d=int(input("ENTER THE NUMBER:"))
s=input("OPERATIPON:")
if(s=="+"):
    print("Answer=",n+d)
elif(s=="-"):
    print("Answer=",n-d)
elif(s=="*"):
    print("Answer=",n*d)
elif(s=="/"):
    print("Answer=",n/d)
elif(s=="//"):
    print("Answer=",n//d)
elif(s=="%"):
    print("Answer=",n%d)
